{
  "name": "suka-back",
  "version": "1.0.0",
  "description": "SukaBack - Backend oficial para SukaDex con NestJS, TypeScript, PostgreSQL y JWT",
  "author": "SukaDex Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:test": "node utils/test-connection.js",
    "db:fix-items": "node utils/fix-item-constraint.js",
    "seed:help": "node seeders/pokeapi-complete.js --help",
    "seed:complete": "node seeders/pokeapi-complete.js",
    "seed:complete:full": "node seeders/pokeapi-complete.js --full",
    "seed:complete:gen1": "node seeders/pokeapi-complete.js --gen=1",
    "seed:complete:gen2": "node seeders/pokeapi-complete.js --gen=2",
    "seed:complete:gen3": "node seeders/pokeapi-complete.js --gen=3",
    "seed:complete:gen4": "node seeders/pokeapi-complete.js --gen=4",
    "seed:complete:gen5": "node seeders/pokeapi-complete.js --gen=5",
    "seed:complete:gen6": "node seeders/pokeapi-complete.js --gen=6",
    "seed:complete:gen7": "node seeders/pokeapi-complete.js --gen=7",
    "seed:complete:gen8": "node seeders/pokeapi-complete.js --gen=8",
    "seed:complete:gen9": "node seeders/pokeapi-complete.js --gen=9",
    "seed:gens:1-3": "node seeders/pokeapi-complete.js --gens=1,2,3",
    "seed:gens:4-6": "node seeders/pokeapi-complete.js --gens=4,5,6",
    "seed:gens:7-9": "node seeders/pokeapi-complete.js --gens=7,8,9",
    "db:import-csv": "node database/import-from-csv.js",
    "db:custom-tables": "node database/create-custom-tables.js",
    "db:monitor": "node utils/monitor-progress.js",
    "db:setup-wizard": "node database/setup-wizard.js",
    "query": "node utils/pokemon-query.js",
    "query:stats": "node utils/pokemon-query.js stats"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "helmet": "^7.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
